<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:FocusApp.Views"
       x:Class="FocusApp.AppShell">
    <TabBar>
        <Tab Title="Schedule"
        >
            <ShellContent ContentTemplate="{DataTemplate views:ScheduleListView}" />
        </Tab>
        <Tab Title="Planner"
        >
            <ShellContent ContentTemplate="{DataTemplate views:TaskView}" />
        </Tab>
    </TabBar>
</Shell>