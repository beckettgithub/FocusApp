<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FocusApp.MainPage">

    <VerticalStackLayout
            Padding="30,0"
            Spacing="25" BackgroundColor="#FFFFFF" Background="#FFFFFF" IsVisible="True">


        <Label Text="" TextColor="Black" />
        <Label
                Text="Welcome to &#10; Focus"
                Style="{StaticResource SubHeadline}"
                SemanticProperties.HeadingLevel="Level2"
                SemanticProperties.Description="Welcome to Focus" />

        <Button Text="ENTER" Background="#000000" BackgroundColor="#000000" TextColor="#FFFFFF" />
    </VerticalStackLayout>

</ContentPage>
